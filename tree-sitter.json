{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tny",
      "camelcase": "Tny",
      "title": "Tny",
      "scope": "source.tny",
      "file-types": [
        "tny"
      ],
      "injection-regex": "^tny$",
      "class-name": "TreeSitterTny"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tny grammar for tree-sitter",
    "authors": [
      {
        "name": "Liam Yates",
        "email": "liamyates2010@icloud.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tny"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}